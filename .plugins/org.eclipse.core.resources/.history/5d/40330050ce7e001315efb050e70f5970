package Controller;

import view.GridView;

public class Controller
{
	private int[][] FavoriteNumbers;
	private Controller ConsoleView;
	private GridView gridView;
	
	public Controller()
	{
		 FavoriteNumbers = new int [4][4];
	}
	
	public String[][] getFavoriteNumbers()
	{
		return getFavoriteNumbers();
	}

	public void setMyFavoriteNumbers(int[][] myFavoriteNumbers)
	{
		this.FavoriteNumbers = myFavoriteNumbers;
	}
	
	public void fillNumbers()
	{
	FavoriteNumbers[0][0]= 25;
	FavoriteNumbers[0][1]= 25;
	FavoriteNumbers[0][2]= 25;
	FavoriteNumbers[0][3]= 25;
	
	FavoriteNumbers[1][0]= 25;
	FavoriteNumbers[1][1]= 25;
	FavoriteNumbers[1][2]= 25;
	FavoriteNumbers[1][3]= 25;
	
	FavoriteNumbers[2][0]= 25;
	FavoriteNumbers[2][1]= 25;
	FavoriteNumbers[2][2]= 25;
	FavoriteNumbers[2][3]= 25;
	
	FavoriteNumbers[3][0]= 25;
	FavoriteNumbers[3][1]= 25;
	FavoriteNumbers[3][2]= 25;
	FavoriteNumbers[3][3]= 25;
	}


	public void start()
	{
		fillNumbers();
		gridView.printStringInformation(FavoriteNumbers);
		
	}
}