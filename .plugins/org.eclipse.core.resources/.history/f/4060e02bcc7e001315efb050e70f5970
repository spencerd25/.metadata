package view;

import Controller.Controller;

public class GridView
{

	
	public void printStringInformation(int FavoriteNumbers)
	{
		System.out.println("foreach printing");
		for(String [] currentRow : FavoriteNumbers)
		{
			for(String currentNumber : currentRow)
			{
				System.out.print(currentNumber + "\t");
			}
			System.out.println();
		}
		
		System.out.println("for printing");
		for(int row = 0; row < FavoriteNumbers.length; row++)
		{
			for(int col = 0; col < FavoriteNumbers[0].length; col++)
			{
				System.out.print(row + "," + col + FavoriteNumbers[row][col] );
			}
		System.out.println();
		}
	}
}
