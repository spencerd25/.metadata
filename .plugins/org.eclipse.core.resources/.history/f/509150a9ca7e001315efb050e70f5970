package Controller;

import java.util.jar.Attributes.Name;

public class Controller
{

	public void setMyFavoriteNumbers(int[][] myFavoriteNumbers)
	{
		this.myFavoriteNumbers = myFavoriteNumbers;
	}
	
	public void fillNumbers()
	{
	myFavoriteNumbers[0][0]= 25;
	myFavoriteNumbers[0][1]= 25;
	myFavoriteNumbers[0][2]= 25;
	myFavoriteNumbers[0][3]= 25;
	
	myFavoriteNumbers[1][0]= 25;
	myFavoriteNumbers[1][1]= 25;
	myFavoriteNumbers[1][2]= 25;
	myFavoriteNumbers[1][3]= 25;
	
	myFavoriteNumbers[2][0]= 25;
	myFavoriteNumbers[2][1]= 25;
	myFavoriteNumbers[2][2]= 25;
	myFavoriteNumbers[2][3]= 25;
	
	myFavoriteNumbers[3][0]= 25;
	myFavoriteNumbers[3][1]= 25;
	myFavoriteNumbers[3][2]= 25;
	myFavoriteNumbers[3][3]= 25;
	}


public void start()
	{
	fillNumbers();
	consoleView.printIntInformation(myFavoriteNumbers);
	}
}